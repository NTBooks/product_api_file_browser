{
    "name": "product-api-file-browser",
    "version": "1.0.0",
    "description": "Modern file browser with React frontend and Express backend",
    "main": "index.js",
    "scripts": {
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "cd server && npm run dev",
        "client": "cd client && npm run dev",
        "build": "cd client && npm run build",
        "install-all": "npm install && cd client && npm install && cd ../server && npm install"
    },
    "keywords": [
        "file-browser",
        "react",
        "express",
        "material-ui"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "concurrently": "^8.2.2"
    },
    "dependencies": {
        "react-intersection-observer": "^9.16.0"
    }
}
